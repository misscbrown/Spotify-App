    "dotenv": "^16.0.1",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.2"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "framer-motion": "^6.3.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
